<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global" >
    <aura:attribute name ="parentrecord" type ="account[]"/>
    <aura:attribute name ="columns" type = "account[]"/>
       <aura:attribute name ="duplicaterecord" type ="account[]"/>
    <aura:attribute name ="jsonn" type ="account[]"/>
   <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    
    
    
    
    <section role="dialog" tabindex="-1" aria-modal="true" aria-labelledby="modal-heading-01" class="slds-modal slds-fade-in-open slds-modal_full">
<div class="slds-modal__container">
<button class="slds-button slds-button_icon slds-modal__close slds-button_icon-border-filled slds-modal_full-close-button">

<span class="slds-assistive-text">Cancel and close</span>
</button>
<div class="slds-modal__header">
<h1 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Modal header</h1>
</div>
<div class="slds-modal__content slds-modal_full-content slds-p-around_medium" id="modal-content-id-1">

            <lightning:datatable
                keyField="id"
                data="{! v.duplicaterecord }"
                columns="{! v.columns }"
                hideCheckboxColumn="false"
                onrowselection="{! c.updateSelectedText }"
                onrowaction="{! c.handleRowAction }"/>
</div>
<div class="slds-modal__footer">
<button class="slds-button slds-button_neutral" aria-label="Cancel and close">Cancel</button>
<button class="slds-button slds-button_brand">Save</button>
</div>
</div>
</section>
<div class="slds-backdrop slds-backdrop_open" role="presentation"></div>
    
    
    
    
    
    
    
    
    
    
    
        
</aura:component>