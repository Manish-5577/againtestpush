<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global" >
    <aura:attribute name ="meetingroominfos" type ="List"/>
    <aura:attribute name ="selectedmeetingRoom" type ="string"/>
     <aura:attribute name ="selectedmeetingRoomcapacity" type ="string"/>
    <aura:handler name ="init" value="{!this}" action ="{!c.doinit}"/>
    <lightning:card title = "Meeting Rooms">
        <lightning:layout>
            <lightning:layoutItem size="4" padding="around-small">               
                <ul>
                    <aura:iteration  items ="{!v.meetingroominfos}" var ="item">
                        <li style="padding :10px">
                            <c:publicpropertyrevision meetingroominfo="{!item}"  booleaneg ="true" onanyeventname="{!c.handlechildlwcevent}"></c:publicpropertyrevision>   
                        </li>                           
                    </aura:iteration>                    
                </ul>   
            </lightning:layoutItem> 
            <lightning:layoutItem size="8" padding="around-small">                
                you have selected : {!v.selectedmeetingRoom}<br> </br>
                you have selected : {!v.selectedmeetingRoomcapacity}<br> </br>
            </lightning:layoutItem>        
        </lightning:layout>
    </lightning:card>
</aura:component>