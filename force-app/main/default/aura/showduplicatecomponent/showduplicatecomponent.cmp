<aura:component controller="duplicateLead" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global" >
    <aura:attribute name ="parent" type ="list"/>
      <aura:attribute name ="childrecord" type ="list"/>
       <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler event="force:refreshView" action="{!c.doInit}"/>
    <aura:attribute name ="duplicate" type ="boolean" default ="false"/>
     <aura:attribute name ="showduplicaterecord" type ="boolean" default ="false"/>
    <aura:if isTrue ="{!v.duplicate}">
    <div class="slds-notify slds-notify_alert slds-alert_warning" role="alert">
<span class="slds-assistive-text">warning</span>
<span class="slds-icon_container slds-icon-utility-warning slds-m-right_x-small" title="Description of icon when needed">

</span>
<h2>Duplicate Lead Exist
<button class="slds-button slds-button_neutral" onclick ="{!c.showduplicate}">Brand Button</button>
</h2>
<div class="slds-notify__close">
<button class="slds-button slds-button_icon slds-button_icon-small" title="Close">

<span class="slds-assistive-text">Close</span>
</button>
</div>
</div>
    </aura:if>
    <aura:if isTrue = "{!v.showduplicaterecord}"  >
        <c:showduplicatecomponent2  duplicaterecord ="{!v.childrecord}"/>
    </aura:if>
    <lightning:button variant ="SUCCESS" label ="checksettimeout"  onclick ="{!c.testgetcallback}"/>
</aura:component>